{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2016",
    "lib": ["es2016"],
    "sourceMap": true,
    "outDir": "lib",
    "rootDir": "src",
    "strict": true,
    "noUnusedLocals": false,
    "noImplicitReturns": true,
    "noUnusedParameters": false
  },
  "exclude": ["lib", "e2e", "node_modules", "test-workspace"]
}
